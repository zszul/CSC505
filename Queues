
public class Queues {
	int queueLength(int min, int max) { // randomizing queue length
        return (int) (Math.random() * (max - min + 1)) + min;
    }

	int longQueue = queueLength(250,350);
	
	int shortQueue = queueLength(20,40);
	
	int mediumQueue = queueLength(100,150);
	
	AQueue longQ = new AQueue(longQueue);
	AQueue shortQ = new AQueue(shortQueue);
	AQueue midQ = new AQueue(mediumQueue);
	
	
	

}
